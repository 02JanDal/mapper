if [ ! "@APPLE@" = "" ]
then
	DYLD_LIBRARY_PATH='@INSTALL_DIR@@QT5_BINARY_DESTINATION@/lib' '@INSTALL_DIR@@QT5_BINARY_DESTINATION@/bin/'$(basename $0) "$@"
elif [ ! "@UNIX@" = "" ]
then
	LD_LIBRARY_PATH='@INSTALL_DIR@@QT5_BINARY_DESTINATION@/lib' '@INSTALL_DIR@@QT5_BINARY_DESTINATION@/bin/'$(basename $0) "$@"
else
	exit 1
fi

